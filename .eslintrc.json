{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [ "import" ],
    "rules": {
      "no-underscore-dangle": "off",
      "no-param-reassign": "off",
      "no-fallthrough": "off",
      "import/no-cycle": "off",
      "max-len": ["error", 120]
    }
}
